<div class="sidetap">
  <div class="stp-nav">
    <div>
      <nav>
        <div class="stp-browse"></div><a href="#browse" class="selected"><%= t ".browse" %></a>
        <div class="stp-viewed"></div><a href="#notifications"><%=  t ".viewed_you" %> </a>
        <div class="stp-likes"></div><a href="#my_likes"><%= t ".my_likes" %></a>
        <div class="stp-hotfriends"></div><a href="#hot_friends"><%= t ".hot_friends" %></a>
        <div class="stp-messages"></div><a href="#messages"><%= t ".messages" %></a>
      </nav>
    </div>
  </div>

  <div class="stp-content" id="content">
    <header class="stp-fake-header">&nbsp;</header>
    <div class="stp-overlay nav-toggle">&nbsp;</div>

    <!-- Content Divs Look Like This -->
    <div class="stp-content-panel" id="browse">
      <header>
        <a href="javascript:void(0)" class="header-button menu"><span>Menu</span></a>
        <h1>Chinchin</h1>
      </header>
      <div class="stp-content-frame">
        <div class="stp-content-body">
          <div class="container">
          </div>
        </div>
      </div>
    </div>
    <!-- End content Div -->

    <div class="stp-content-panel hidden" id="chinchin">
      <header>
        <a href="javascript:void(0)" class="icon back"><span>Back</span></a>
        <h1></h1>
        <a href="javascript:void(0)" class="icon like right"><span>Like</span></a>
      </header>
      <div class="stp-content-frame">
        <div class="stp-content-body">
          <div class="container">
          </div>
        </div>
      </div>
    </div>

    <div class="stp-content-panel hidden" id="message_room">
      <header>
        <a href="javascript:void(0)" class="icon back"><span>Back</span></a>
        <h1></h1>
      </header>
      <div class="stp-content-frame">
        <div class="stp-content-body">
          <div class="container">
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- start Mixpanel Chinchin List -->
<script type="text/javascript">
  mixpanel.identify(<%= current_user.id %>);
  mixpanel.people.set({
    "$name": "<%= current_user.name %>",
    "$email": "<%= current_user.email %>",
    "$created": "<%= current_user.created_at %>",
    "$gender": "<%= current_user.gender %>",
    "$last_login": new Date()
  });
  mixpanel.people.increment("Visits");
  mixpanel.register({"Gender": "<%= current_user.gender %>"});
</script>
<!-- end Mixpanel Chinchin List -->