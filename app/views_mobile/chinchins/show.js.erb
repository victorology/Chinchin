mixpanel.track("View Chinchin Profile");
<% if @user.liked(@chinchin) %>
    $('#chinchin a.like').addClass('liked');
<% end %>
var html =
<% cache ["show", @chinchin] do %>
    "<%= j render(:partial => 'chinchin', :locals => {:chinchin => @chinchin}) %>"
<% end %>
$('#chinchin').find('.container').html(html);